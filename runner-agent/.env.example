# Runner Agent Configuration
# Copy this file to .env and fill in your values

# REQUIRED: Authentication Token
# Get this by running /generate-token in Discord
DISCORDE_TOKEN=your_token_from_discord_bot_here

# OPTIONAL: Discord Bot WebSocket URL
# Default: ws://localhost:8080
DISCORDE_BOT_URL=ws://localhost:8080

# OPTIONAL: Runner Name
# Default: local-runner
DISCORDE_RUNNER_NAME=my-macbook-pro

# OPTIONAL: CLI Type
# Values: claude, gemini
# Default: claude
DISCORDE_CLI_TYPE=claude

# OPTIONAL: HTTP Server Port for CLI Plugin
# Default: 3000
DISCORDE_HTTP_PORT=3000
